/* components/footerNav.wxss */
.footer-nav{
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  box-shadow: 0px -5px 35px rgba(0, 0, 0, 0.15);
}

.fix-iphonex{
  bottom: 50rpx!important;
}
.fix-iphonex::after {
  content: ' ';
  position: fixed;
  left: 0;
  bottom: 0!important;
  height: 50rpx!important;
  width: 100%;
  background: #fff;
}

.footer-li{
  float: left;
  width: 25%;
  height: 100rpx;
  text-align: center;
  font-size: 24rpx;
  color: #838383;
}
.footer-li.on{
  color: #ff7841;
}
.icon-footer{
  width: 40rpx;
  height: 40rpx;
  background: #000;
  margin: 18rpx auto 0 auto;
}

/* 图片转base64 */
.icon-footer.index{
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.icon-footer.attention{
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.icon-footer.fair{
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.icon-footer.person{
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}

/* 选中状态 */
.footer-li.on .icon-footer.index{
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}

