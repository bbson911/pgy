/* pages/red_record/red_record.wxss */
page{
  background: #f2f2f2;
  padding-bottom: 100rpx;
}
.red_person{
  width: 750rpx;
  height: 340rpx;
  background: url(data:image/jpeg;base64,)no-repeat;
  background-size:100%;
  padding-top: 33rpx;
  text-align: center;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  position: relative;
}

.red_person > image{
  width: 137rpx;
  height: 137rpx;
}
.red_person .person_names{
   font-family: Arial;
   margin: 0;
   color: #ffffff;
   font-size: 28rpx;
   margin-top: 10rpx;
}
.red_person_detail{
width: 690rpx;
height: 40rpx;
line-height: 40rpx;
font-size: 24rpx;
color: #fffce7;
position: absolute;
bottom: 24rpx;
left: 30rpx;

}
.get_lefts{
  float: left;
}
.get_rights{
  float: right;
}
.price_symbol{
  font-size: 20rpx;
}
.price_money{
  font-size: 28rpx;
}
/*红包记录列表  */
.red_recordList{
  width: 100%;
  height: auto;
  margin-top: 10rpx;
}
.g_recordList{
  height: 100rpx;
  border-bottom: 1rpx solid #eaeaea;
  background: #ffffff;
  box-sizing: border-box;
  padding: 20rpx 30rpx 22rpx 30rpx;
}
.g_recordleft{
  float: left;
  position: relative;
}
.g_recordright{
  float: right;
  font-size: 30rpx;
  line-height: 30rpx;
  height: 30rpx;
  color: #373737;
  margin-top: 15rpx;
}
.g_recordright text{
  font-size: 22rpx;
}
.record_name{
  font-size: 24rpx;
  color: #373737;
  line-height: 24rpx;
  height: 24rpx;

}
.record_times{
  color: #7e7e7e;
  font-size: 24rpx;
  line-height: 24rpx;
  height: 24rpx;
  margin-top: 13rpx;

}