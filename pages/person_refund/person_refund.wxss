/* pages/icome_account/icome_account.wxss */
page{
  background: #f2f2f2;
  padding-bottom: 100rpx;
}
.get_pagfor{
  background: -webkit-linear-gradient(top, #ff7841, #ff5941); 
  width:100%;
  height: 210rpx;
  text-align: center;
  position: relative;
  color: #ffffff;
  font-size: 34rpx;
  padding-top: 40rpx;
  box-sizing: border-box;
}
.get_pagfor image{
  position: absolute;
  top: 20rpx;
  right: 49rpx;
  width: 166rpx;
  height: 132rpx;
}
.pagfor_money{
  font-size: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 27rpx;
}
.pagfor_money text{
  font-size: 30rpx;
}
/*收益详情  */
.pagfor_details{
  margin-top: 10rpx;
}
.pagfor_details >view{
  background: #ffffff;
}
.pagfor_detailList{
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1px solid #eaeaea;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
}
.pagfor_lefts{
  float: left;
  color: #373737;
  font-size: 28rpx;
}
.pagfor_rights{
 float: right;
  color: #373737;
  font-size: 28rpx;
}
/* 联系客服 */
.concat_chat{
  width: 175rpx;
  height: 33rpx;
  line-height: 33rpx;
position: fixed;
bottom: 50rpx;
left: 290rpx;

}
.concat_chat >image{
  margin-right: 30rpx;
  width: 31rpx;
  height: 31rpx;
  float: left;
}
.concat_chat >view{
  float: right;
  font-size: 28rpx;
  color: #373737;

}