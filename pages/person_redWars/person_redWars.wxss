/* pages/red_wars/red_wars.wxss */
page{
  background: #f2f2f2;
  padding-bottom: 100rpx;
}

.red_commodity{
  width: 750rpx;
  height: 340rpx;
  background: url(data:image/jpeg;base64,)no-repeat;
  background-size:100%;
  padding-top: 66rpx;
  text-align: center;
}
.red_name{
  font-size: 38rpx;
  color: #ffffff;
  
}
.red_price{
  font-size: 48rpx;
  color: #fffce7;
  margin-top: 65rpx;
}
.red_price text{
  font-size: 82rpx;
}
.get_red{
  background: #f2f2f2;
  height: 102rpx;
  line-height: 102rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #373737;
}
/*红包列表  */
.income_list{
  width: 100%;
  height: 120rpx;
  border-bottom: 1rpx solid #eaeaea;
  padding: 26rpx 30rpx;
  background: #ffffff;
  position: relative;
}
.incomeList_pic{
  width: 68rpx;
  height: 68rpx;
  float: left;

}
.income_list_left{
  width: 230rpx;
}
.income_list_right{
   color: #ff4141;
  font-size: 24rpx;
  text-align: right;
  
}
.income_list_right >.adminMoney{

  font-size: 30rpx;
  line-height: 30rpx;
  height: 30rpx;
   color: #373737;
   position: absolute;
   right: 30rpx;
   bottom: 20rpx;
}
.adminMoney> text{
font-size: 22rpx;
}
.income_list_left{
  float:left;
}
.incomeList_admin{
  font-size: 24rpx;
  color: #373737;
  float: right;
  line-height: 32rpx;
  height: 68rpx;

}
.incomeList_admin .adminWraps{
 color: #373737;
 margin-top: 8rpx;
 height: 24rpx;
 line-height: 24rpx;

}
.incomeList_pic image{
   width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
}
.redList{
  width: 100%;
  height:auto;
}
.next_redwars{
  margin-top: 131rpx;
}
.show_red{
  width: 198rpx;
  height: 68rpx;
  border-radius: 40rpx;
  border: 1px solid #898989;
  font-size: 34rpx;
  color: #373737;
  text-align: center;
  line-height: 68rpx;
  margin: 0 auto;

}
.getNext_red{
  text-align: center;
  margin-top: 42rpx;
  font-size: 24rpx;
  color: #373737;
}


/*抢红包  */
.modal_redwars{
  width:100%;
  background:rgba(0,0,0,0.2);
  height:100%;
  position:fixed;
  top:0rpx;
  left:0rpx;
  z-index:999;
}
.click_redwars{
  width: 534rpx;
  height: 754rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -267rpx;
  margin-top:-377rpx;
  text-align: center;
  
}
.click_redwars .red_image{
  width: 534rpx;
  height: 754rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -267rpx;
  margin-top:-377rpx;

}
.click_redwars .red_image image{
   width: 534rpx;
  height: 754rpx;
  display: block;
}
