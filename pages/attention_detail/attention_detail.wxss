/* pages/attention_detail/attention_detail.wxss */
page{
  background: #f2f2f2;
 
}
/*关注 文章列表*/

.attention_list{
  width: 100%;
  height: 205rpx;
  background: #ffffff;
  padding: 30rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eaeaea;
  margin-top: 10rpx;
}
.attention_left_body{
  float:left ;
  width: 485rpx;
  height: 144rpx;
  position: relative;

}
.attention_left_content{
   position: absolute;
    height: 35rpx;
    line-height: 35rpx;
    bottom: 0rpx;
 
}
.attention_right_body{
  float: right;
  width: 190rpx;
  height: 144rpx;
  background-size: cover;
  background-position: center;
}

.attention_left_title{
font-size: 28rpx;
line-height: 38rpx;
color: #373737;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
}
.icon-img:nth-of-type(1){
  margin: 8rpx 5rpx auto 0rpx;
}
.icon-img:nth-of-type(2){
  margin: 8rpx 5rpx auto 36rpx;
  width: 33rpx;
  height: 25rpx;
  position:relative;
  top: 3rpx;
}
.icon-img{
  width: 33rpx;
  height: 25rpx;
  margin: 8rpx 5rpx auto 36rpx;
  position:relative;
  top: 4rpx;
}

.attention_left_content text{
  font-size: 24rpx;
  color: #373737;
}
/*我的收益列表  */
.attention_income{
  margin-top: 10rpx;
}
.income_title{
  height: 70rpx;
  line-height: 70rpx;
  width: 100%;
  padding: 0rpx 30rpx;
  border-bottom: 1rpx solid #eaeaea;
  background: #ffffff;
}
.income_title >view{
  font-size: 28rpx;
  color: #373737;
}
.income_title_right,.income_list_right{
  float: right;
}
.income_title_right{
  padding-right: 38rpx;
  width: 162rpx;
  background: url(data:image/png;base64,)right no-repeat;
  background-size: 22rpx 14rpx;
  text-align: right;
}
.income_title_left,.income_list_left{
  float:left;
}
.income_list{
  width: 100%;
  height: 120rpx;
  border-bottom: 1rpx solid #eaeaea;
  padding: 26rpx 30rpx;
  background: #ffffff;
}
.incomeList_pic{
  width: 68rpx;
  height: 68rpx;
  float: left;

}
.income_list_left{
  width: 230rpx;
}
.income_list_right{
  color: #373737;
  font-size: 24rpx;
  text-align: right;
}
.income_list_right .adminMoney{
  color: #ff4141;
  font-size: 30rpx;
}
.incomeList_pic image{
   width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
}
.incomeList_admin{
  font-size: 24rpx;
  color: #373737;
  float: right;
  line-height: 32rpx;
  height: 68rpx;
  margin-top: 6rpx; 
}
.incomeList_admin .adminWraps{
 color: #373737;
}
/*更多收益信息  */
.footer_more{
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  color: #373737;
  font-size: 28rpx;
  background: #ffffff;
}

/*弹出层  */
.picker_titles{
  height:100rpx;
  line-height:100rpx;
  border-bottom:1rpx solid #eaeaea;
  width:100%;
  position:absolute;
  z-index:100000;
  bottom:363rpx;
  background:#ffffff;
}
.navigator_left{
  position:absolute;
  left:30rpx;
  font-size:34rpx;
  color:#373737;
}
.navigator_right{
  position:absolute;
  right:30rpx;
  font-size:34rpx;
  color:#ff7841;
  border-radius: 0rpx;
}
.navigator_center{
  position:absolute;
  text-align: left;
  left: 324rpx;
  font-size:34rpx;
  color:#373737;
  width: 103rpx;
}
.movable_viewstyle{
  width:100%;
  background:rgba(0,0,0,0.3);
  height:100%;
  position:fixed;
  top:0rpx;
  left:0rpx;
  z-index:999;
}
.choise_value{
  height:80rpx;
  line-height:75rpx;
  border:none;
  font-size:30rpx;
  width:100%;
  display:inline-block;
}
.noAbout {
  width: 100%;
  min-height: 920rpx;
  background: #ffffff;
  overflow: hidden;
  text-align: center;
}
.noAbout image{
  margin:212rpx auto auto auto;
  width:270rpx;
  height: 230rpx; 
  display: block;
}
.noAbout text{
  text-align: center;
  font-size: 28rpx;
  color: #373737;
}